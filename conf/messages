since.hours=il y a %dH
since.days=il y a %d jours
since.months=il y a %d mois
since.years=il y a %d ans

# Validation errors
validation.errors=Quelques erreurs sont à corriger dans ta saisie mon ami !
validation.required=La saisie est obligatoire
validation.email=Ce n'est pas un email valide
validation.minSize=La longueur minimale est %2$d
validation.maxSize=La longueur maximale est %2$d
validation.url=Ce n'est pas une URL valide
validation.unique=La valeur du champ %s est déjà utilisée par l'utilisateur %s
validation.googleID=Ce n'est pas à un Google ID valide